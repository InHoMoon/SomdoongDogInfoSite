<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="somdoong.admin.dao.face.AdminDao">
    
    
    <!-- 로그인 관련 mapper-->
    <select id = "login_check" parameterType=
    "somdoong.admin.dto.Admin"
    resultType="String">
        select admin_id from admin
        where admin_id=#{admin_id} and admin_pass=#{admin_pass}
    </select>    
 
 
    <!-- 회원 강제탈퇴관련 mapper -->
    <delete id="admin_member_forced_evictionCheck">
    delete from member
    where user_id=#{user_id}
    </delete>
    
 
</mapper>